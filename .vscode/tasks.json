{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe 生成活动文件",
            "command": "E:\\mingw64\\bin\\g++.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-O3",
                "-g",
                "${fileDirname}\\*.cpp",
                "-g",
                "${workspaceFolder}/src/eskf/*.cpp",
                "-g",
                "${workspaceFolder}/src/leskf/*.cpp",
                "-g",
                "${workspaceFolder}/src/geskf/*.cpp",
                "-g",
                "${workspaceFolder}/src/liekf/*.cpp",
                "-g",
                "${workspaceFolder}/src/riekf/*.cpp",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",
                "-I",
                "E:/eigen3",
                "-I",
                "${workspaceFolder}/include/eskf",
                "-I",
                "${workspaceFolder}/include/leskf",
                "-I",
                "${workspaceFolder}/include/geskf",
                "-I",
                "${workspaceFolder}/include/liekf",
                "-I",
                "${workspaceFolder}/include/riekf"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}